{
  "name": "speed-dial-firefox",
  "author": {
    "name": "Phuoc Le"
  },
  "verion": "1.0.0",
  "description": "An extension to get the Opera Speed Dial to Firefox",
  "keywords": [
    "firefox",
    "speed dial",
    "toolbar dial"
  ],
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.1.0",
    "@mui/material": "^5.1.0",
    "@mui/system": "^5.1.0",
    "notistack": "^2.0.3",
    "react": "^17.0.2",
    "react-color": "^2.18.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^4.9.8",
    "react-sortable-hoc": "^2.0.0",
    "react-use": "^13.26.2",
    "typeface-roboto": "0.0.75",
    "uuid": "^3.4.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:parcel": "parcel build src/manifest.json --no-cache --no-autoinstall",
    "build:webext": "web-ext build",
    "watch:parcel": "parcel watch src/manifest.json --no-autoinstall --no-hmr",
    "watch:webext": "web-ext run --source-dir ./dist/",
    "build": "run-s clean build:parcel build:webext",
    "develop": "npm-run-all clean build:parcel -plr watch:parcel watch:webext"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@parcel/config-default": "^2.0.1",
    "@parcel/config-webextension": "^2.0.1",
    "@parcel/transformer-typescript-tsc": "^2.0.1",
    "@types/firefox-webext-browser": "^70.0.1",
    "@types/react": "^16.9.22",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.9.5",
    "@types/uuid": "^3.4.7",
    "babel-plugin-emotion": "^11.0.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0",
    "parcel-plugin-web-extension": "^1.6.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.8.2",
    "web-ext": "^4.3.0"
  },
  "browserslist": [
    "last 2 Firefox versions"
  ]
}
